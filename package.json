{
  "name": "api-codegen-runner",
  "version": "0.0.0",
  "description": "Generate API code from standardized JSON using templates",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "api-codegen-runner": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "api-codegen-universal": "^0.3.0",
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "ejs": "^3.1.10",
    "fs-extra": "^11.3.2",
    "inquirer": "^13.0.1",
    "install": "^0.13.0",
    "prettier": "^3.7.3",
    "unconfig": "^7.4.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.10.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
